<% include header %>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" autocomplete="off">
    <label>Post</label>
    <textarea class="form-control"  name="postBody" rows="5" cols="30" autocomplete="off"></textarea>
  </div>
  <button class="btn btn-primary"type="submit" name="button">Publish</button>

</form>


